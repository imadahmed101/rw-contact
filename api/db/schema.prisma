datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model Contact {
  id        Int       @id @default(autoincrement())
  name      String
  email     String    @unique
  comment   String
  courses   String?
  createdAt DateTime  @default(now())
}
